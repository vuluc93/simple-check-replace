{
  "name": "call-openai",
  "displayName": "call-openai",
  "description": "VS Code extension call-openai",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.setOpenAIKey",
        "title": "Set OpenAI API Key"
      },
      {
        "command": "extension.callOpenai",
        "title": "Ask OpenAI"
      }
    ],
    "keybindings": [
      {
        "command": "extension.callOpenai",
        "key": "ctrl+alt+/",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "openai": "^6.9.0"
  }
}
